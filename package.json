{
  "name": "platform2.0-qa-em-challenge",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "proxy": "http://localhost:3001",
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@shelex/cypress-allure-plugin": "^2.26.5",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "cypress": "^6.2.0",
    "cypress-dashboard": "^3.1.4",
    "cypress-wait-until": "^1.7.2",
    "date-fns": "^2.25.0",
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "ts-node": "^10.7.0",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "cypress": "cypress cache clear",
    "report:merge": "mochawesome-merge cypress/results/json/*.json -o cypress/results/mochawesome-bundle.json",
    "report:generate": "marge cypress/results/mochawesome-bundle.json -o cypress/reports/html",
    "report:copyScreenshots": "cp -r images cypress/reports/html/screenshots",
    "cleanup": "rm -rf cypress/results && rm -rf cypress/reports && rm -rf images",
    "cy:run:firefox": "cypress run --spec cypress/integration/gwi/**/*.spec.js --browser firefox",
    "cy:run:chrome": "cypress run --spec cypress/integration/gwi/**/*.spec.js --browser chrome",
    "react": "react-scripts start",
    "server": "node --experimental-specifier-resolution=node --loader ts-node/esm server/index.ts",
    "start": "npm run server & npm run react",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@bahmutov/cy-api": "^2.1.3",
    "@types/express": "^4.17.13",
    "@types/node": "^12.20.52",
    "concurrently": "^6.3.0",
    "cypress-multi-reporters": "^1.6.0",
    "mocha": "^10.0.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "typescript": "^4.6.4"
  }
}
